{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CARLA": {
    "Repositories": "https://github.com/carla-simulator/carla.git"
  },
  "Python": {
    "Target": "https://docs.python.org/release/"
  },
  "VersionCapture": {
    "SleepMillisecond": 1200000
  }
}
